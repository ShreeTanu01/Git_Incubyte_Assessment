<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">
<groups>
  <run>
   <include name= "ErrorHandling"/>
  </run>
</groups>


  <test thread-count="5" name="Sign In Test">
    <classes>
      <class name="IncubyteWebAutomation.Tests.SelectSignIn"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Create Account Test">
    <classes>
      <class name="IncubyteWebAutomation.Tests.SelectCreateAccount"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="IncubyteWebAutomation.Tests.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
